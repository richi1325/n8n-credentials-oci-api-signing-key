{
  "name": "n8n-credentials-oci-api-signing-key",
  "version": "0.1.0",
  "description": "A n8n credential type for Oracle Cloud Infrastructure API Signing Key",
  "keywords": [
    "oci",
    "oracle",
    "n8n",
    "n8n-community",
    "n8n-community-credentials-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/richi1325/n8n-credentials-oci-api-signing-key.git",
  "author": {
    "name": "Jose Ricardo Mendoza Villar",
    "email": "ricardo.mendoza@oracle.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/richi1325/n8n-credentials-oci-api-signing-key.git"
  },
  "main": "dist/credentials/OciAPISigningKey.credentials.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write credentials/**/*.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OciAPISigningKey.credentials.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node-fetch": "^2.6.12",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.2.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.2",
    "typescript": "^5.8.3"
  }
}
